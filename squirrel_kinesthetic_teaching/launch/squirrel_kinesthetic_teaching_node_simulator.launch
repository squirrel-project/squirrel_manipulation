	</include>

	<include file="$(find robotino_bringup_sim)/launch/robot.launch">
		 <arg name="robot" value="robotino"/>
		 <arg name="robot_env" value="simple_room"/>
	</include>

  	<include file="$(find robotinoSH_6dof_moveit_config)/launch/move_group.launch"/>

	<node name="squirrel_kinesthetic_teaching_node" pkg="squirrel_kinesthetic_teaching" type="squirrel_kinesthetic_teaching_node.py" output="screen"/>
</launch>